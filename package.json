{
  "name": "vite-react-static",
  "packageManager": "yarn@4.9.1",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "dev": "unbuild",
    "build": "unbuild",
    "lint": "eslint ."
  },
  "sideEffects": false,
  "bin": "bin/vite-react-static.js",
  "files": [
    "*.d.ts",
    "bin",
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./node": {
      "import": "./dist/node.mjs",
      "require": "./dist/node.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "dependencies": {
    "cac": "^6.7.14",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "eslint": "^9.22.0",
    "globals": "^16.0.0",
    "process": "^0.11.10",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "unbuild": "^3.5.0",
    "vite": "^6.3.1"
  },
  "workspaces": [
    "examples/*"
  ]
}
